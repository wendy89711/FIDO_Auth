@{
    ViewBag.Title = "首頁";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 行動身分驗證</title>
</head>




<body>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap-sketchy.min.css" rel="stylesheet" />

    <div class="navbar navbar-expand-lg fixed-top navbar-light bg-light">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index", "Home")" style="display:inline">
                <img src="~/images/ck.png" width="60" height="53" />
            </a>
            <a class="navbar-brand" href="@Url.Action("Index", "Home")" style="display:inline">
                <img src="~/images/name.png" width="200" height="45" />
            </a>
            @*<a class="nav-link" href="@Url.Action("Index", "Home")" style="font-size:28px">
                       行動身分驗證
                </a>*@
            <span></span>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="nav navbar-nav navbar-right">
                    <li class="nav-item">
                        <button type="button" class="btn btn-warning" style="font-size:16px" onclick="location.href='@Url.Action("Index", "Register")'">登入</button>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <br />
    <br />
    <img src="~/images/home.png" style="width:100%; height:100%; display:block; margin:auto;" />

    <div class="container body-content">
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - 行動身分驗證</p>
        </footer>
    </div>

</body>
</html>



